<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/white"   >    <ImageView        android:layout_width="match_parent"        android:layout_height="400dp"        android:background="@mipmap/mine_bg" />    <com.android.baselibrary.widget.tabLayout.TabLayout        android:id="@+id/tabLayout"        style="@style/style_item_main_tab"        android:layout_width="match_parent"        android:layout_height="40dp"        android:background="@color/transparent" />    <androidx.viewpager.widget.ViewPager        android:id="@+id/viewPager"        android:layout_width="match_parent"        android:layout_below="@id/tabLayout"        android:layout_height="match_parent" /></RelativeLayout>